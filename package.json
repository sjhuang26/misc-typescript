{
  "name": "misc-typescript",
  "devDependencies": {
    "prettier": "^2.2.1",
    "tsc": "^1.20150623.0",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "run-theme": "tsc --rootDir ./src-theme --outDir ./build ./src/index.ts && node ./build/index.js > ./build/out.txt",
    "run-upgrader-game": "tsc --rootDir ./src-upgrader --outDir ./build ./src/index.ts && node ./build/index.js",
    "lint": "npx prettier --write ./src-theme ./src-upgrader"
  },
  "dependencies": {
    "@types/prompt-sync": "^4.1.0",
    "prompt-sync": "^4.2.0"
  }
}
